word a:5;
word b:5;
bool c:true;

word main()
  {
    loop {
    } ( a == b )
    loop {
    } ( a != b )
    loop {
    } ( a > b )
    loop {
    } ( a >= b )
    loop {
    } ( a < b )
    loop {
    } ( a <= b )
    loop {
    } ( ( (a * b) != (b/a) ) || c )
    loop {
    } ( ( (a * b) != (b/a) ) && ( c || !c ) )
  };


// UFRGS - Compiladores - Marcelo Johann - 2014/1

word a:5;
word b:5;
bool c:true;

word main()
  {
    if ( a == b ) then {}
    if ( a != b ) then {}
    if ( a > b ) then {}
    if ( a >= b ) then {}
    if ( a < b ) then {}
    if ( a <= b ) then {}
    if ( ( (a * b) != (b/a) ) || c ) then {}
    if ( ( (a * b) != (b/a) ) && ( c || !c ) ) then {}
  };

